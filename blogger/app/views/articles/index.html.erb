<h1>All Articles</h1>

<ul id="articles">
  <% @articles.each do |article| %>
      <li><%= article.title %></li>
      <li><%= article.body %></li>
      <%= link_to article.title, article_path(article) %>
  <% end %>
</ul>

<%= link_to "Create A New Aticle", new_article_path, class: "new_article"%>
